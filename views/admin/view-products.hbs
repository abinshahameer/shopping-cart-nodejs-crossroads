<section>
    <div class="container">
        <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">description</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.description}}</td>
      <td><img style="width: 50px;height: 50px;" src="{{this.image}}" alt=""></td>
    </tr>
    {{/each}}
   
  </tbody>
</table>
    </div>
</section>